            "Source/ConstTypeArgs.Chars/ConstTypeArgs.Chars.csproj",
            "Source/ConstTypeArgs.Console/ConstTypeArgs.Console.csproj",
            "Source/ConstTypeArgs.Core/ConstTypeArgs.Core.csproj",
            "Source/ConstTypeArgs.Decimals/ConstTypeArgs.Decimals.csproj",
            "Source/ConstTypeArgs.Delegates/ConstTypeArgs.Delegates.csproj",
            "Source/ConstTypeArgs.Doubles/ConstTypeArgs.Doubles.csproj",
            "Source/ConstTypeArgs.Floats/ConstTypeArgs.Floats.csproj",
            "Source/ConstTypeArgs.Halfs/ConstTypeArgs.Halfs.csproj",
            "Source/ConstTypeArgs.Int128s/ConstTypeArgs.Int128s.csproj",
            "Source/ConstTypeArgs.Ints/ConstTypeArgs.Ints.csproj",
            "Source/ConstTypeArgs.Longs/ConstTypeArgs.Longs.csproj",
            "Source/ConstTypeArgs.Nints/ConstTypeArgs.Nints.csproj",
            "Source/ConstTypeArgs.Nuints/ConstTypeArgs.Nuints.csproj",
            "Source/ConstTypeArgs.Sbytes/ConstTypeArgs.Sbytes.csproj",
            "Source/ConstTypeArgs.Shorts/ConstTypeArgs.Shorts.csproj",
            "Source/ConstTypeArgs.Strings/ConstTypeArgs.Strings.csproj",
            "Source/ConstTypeArgs.Types/ConstTypeArgs.Types.csproj",
            "Source/ConstTypeArgs.UInt128s/ConstTypeArgs.UInt128s.csproj",
            "Source/ConstTypeArgs.Uints/ConstTypeArgs.Uints.csproj",
